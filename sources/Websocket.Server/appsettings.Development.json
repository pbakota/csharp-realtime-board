{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDB": {
    "ConnectionString": "mongodb://root:example@playbox:27017",
    "Database": "rt-board"
  },
  "spring": {
    "application": {
      "name": "websocket-server"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false
    },
    "instance": {
      "hostname": "localhost",
      "port": 5000
    }
  },
  "relay": {
    "host": "playbox",
    "port": 61613,
    "enable": [
      "/topic",
      "/queue"
    ],
    "prefix": [
      "/app",
      "/api"
    ]
  }
}